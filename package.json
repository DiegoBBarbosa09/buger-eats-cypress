{
  "name": "buger-eats",
  "version": "1.0.0",
  "description": "Projeto de testes de aceitação do app BugerEats",
  "main": "index.js",
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "cy:run": "eslint && cypress run",
    "cy:open": "eslint && cypress open",
    "cy:run:report": "npx cypress run --reporter mocha-allure-reporter",
    "cy:open:report": "npx cypress open --reporter mocha-allure-reporter",
    "report:allure": "npx allure generate allure-results --clean -o allure-report && npx open allure-report",
    "report:allure:ci": "npx allure generate allure-results --clean -o allure-report"
  },
  "keywords": [],
  "author": "Diego Brito",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@faker-js/faker": "^8.4.1",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "allure-commandline": "^2.30.0",
    "allure-cypress": "^3.0.0-beta.10",
    "cypress": "^13.13.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-terminal-report": "^6.1.2",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-prettier": "^5.2.1",
    "faker": "^5.5.3",
    "gerador-validador-cpf": "^5.0.2",
    "mocha-allure-reporter": "^1.4.0",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^3.3.3"
  }
}
